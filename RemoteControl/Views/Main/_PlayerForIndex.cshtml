@model Camera


<div id="blockscreen300" >
    <a href=@Url.Action("Detail", Model)  class="detailref">
            @Html.Action("BlockScreen", Model) 
    </a>
    </div>
<div id='@Model.Id.GetHashCode()+333' class="thumbnail"></div>

<script type='text/javascript'>
            jwplayer('@Model.Id.GetHashCode()+333').setup({
                file: '@Model.CameraPath',
                image:  'https://i.ytimg.com/vi/ngElkyQ6Rhs/hqdefault.jpg',
                rtmp: {
                    bufferlength: 1
                },
                autostart: 'true',
                width: '100%',
                height: '100%',
               
                aspectratio: '16:9'
            });
</script>

