@model  RemoteControl.Models.Camera

    @{AjaxOptions ajaxOpts = new AjaxOptions()
      {
          UpdateTargetId = "but",
          Url = Url.Action("TakeToQueue", Model)
          
      };
    }
    @using (Ajax.BeginForm(ajaxOpts))
{
    <input id="takeToQueue" type="submit" class="btn btn-success" value="Take to the Queue" name="waiting" />
}
    <div id="blockscreen700">@Html.Action("BlockScreen", Model) </div>  
    @Html.Partial("_Player", new Player { camera = Model, width = 700, height = 500 })



