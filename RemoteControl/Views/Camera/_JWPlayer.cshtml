@model RemoteControl.Models.Camera
<style>
            #blockscreen300 {
                position: absolute;
                z-index: 100;
                width: 300px;
                height: 300px;
                border: 0px solid red;
            }
        </style>
    @*Block Screen*@
    <div id="blockscreen300" >

          <span class="badge badge-danger">@ViewBag.CameraState</span>
       

        @*<a href="#click" onclick="window.location.href='@Url.Action("BigCamera", "Detail", Model)'" >
            12345
        </a> *@
        

</div>
        <div id='@Model.Id'></div>
        <script type='text/javascript'>
            jwplayer('@Model.Id').setup({
                file: '@Model.CameraPath',/*'rtmp://82.209.194.198:1935/live/ipcamera.stream',/* 'rtmp://178.172.245.5:1935/live/livestream', Расскоментировать*/
                image:  'https://i.ytimg.com/vi/ngElkyQ6Rhs/hqdefault.jpg',
                rtmp: {
                    bufferlength: 1
                },
                autostart: 'true',
                width: 300,
                height: 300,
                //  width: '100%',
                aspectratio: '16:9'
            });
        </script>

